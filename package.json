    "express": "^4.17.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.0.3",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
